{
  "name": "@lucid-evolution/experimental",
  "version": "0.0.0",
  "description": "",
  "main": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./Script": {
      "types": "./dist/types/Script.d.ts",
      "require": "./dist/cjs/Script.js",
      "import": "./dist/esm/Script.js"
    },
    "./Network": {
      "types": "./dist/types/Network.d.ts",
      "require": "./dist/cjs/Network.js",
      "import": "./dist/esm/Network.js"
    },
    "./EffectCML/AssetName": {
      "types": "./dist/types/EffectCML/AssetName.d.ts",
      "require": "./dist/cjs/EffectCML/AssetName.js",
      "import": "./dist/esm/EffectCML/AssetName.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.esm.json  && tsc -p tsconfig.cjs.json",
    "test": "vitest run",
    "lint": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browser": {
    "@anastasia-labs/cardano-multiplatform-lib-nodejs": "@anastasia-labs/cardano-multiplatform-lib-browser"
  },
  "dependencies": {
    "@anastasia-labs/cardano-multiplatform-lib-browser": "6.0.2-3",
    "@anastasia-labs/cardano-multiplatform-lib-nodejs": "6.0.2-3",
    "@harmoniclabs/plutus-data": "^1.2.4",
    "@harmoniclabs/uplc": "^1.2.4",
    "@sinclair/typebox": "^0.32.28",
    "cbor-x": "^1.6.0",
    "effect": "^3.13.2"
  },
  "devDependencies": {
    "@effect/vitest": "^0.16.0",
    "@microsoft/api-extractor": "^7.49.0",
    "@types/node": "^20.12.8",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "unbuild": "^3.2.0",
    "vitest": "^2.0.5"
  }
}
